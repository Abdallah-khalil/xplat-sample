{
  "name": "sp-backend-api",
  "version": "1.0.0",
  "private": true,
  "keywords": [],
  "author": "Jeff Whelpley",
  "license": "Apache-2.0",
  "main": "lib/src/api.function.js",
  "scripts": {
    "setup": "npm i -g typescript && npm i || true",
    "test": "jest --config=./test/jest.unit.json || true",
    "test-watch": "jest --watch --config=./test/jest.unit.json || true",
    "test-integration": "jest --runInBand --config=./test/jest.integration.json --forceExit || true",
    "test-e2e": "jest --runInBand --config=./test/jest.e2e.json --forceExit || true",
    "seed": "tsc && node lib/src/job.start.js -n dataload -d || true",
    "exportdata": "tsc && node lib/src/job.start.js -n dataload -x",
    "ssrurls": "tsc && node lib/src/job.start.js -n ssrurls -e production",
    "build": "npm run clean && tsc && npm run copy || true",
    "copy": "cp env.json lib && npm run copy-donation && npm run copy-gathering && npm run copy-love && npm run copy-sketch && npm run copy-sponsored && npm run copy-tag && npm run copy-user || true",
    "copy-donation": "cp src/services/donation/donation.graphql.typedefs.gql lib/src/services/donation || true",
    "copy-gathering": "cp src/services/gathering/gathering.graphql.typedefs.gql lib/src/services/gathering || true",
    "copy-love": "cp src/services/love/love.graphql.typedefs.gql lib/src/services/love || true",
    "copy-sketch": "cp src/services/sketch/sketch.graphql.typedefs.gql lib/src/services/sketch || true",
    "copy-sponsored": "cp src/services/sponsored/sponsored.graphql.typedefs.gql lib/src/services/sponsored || true",
    "copy-tag": "cp src/services/tag/tag.graphql.typedefs.gql lib/src/services/tag || true",
    "copy-user": "cp src/services/user/user.graphql.typedefs.gql lib/src/services/user || true",
    "start": "concurrently \"tsc -w\" \"nodemon lib/src/api.start.js\" || true",
    "clean": "rm -rf lib && mkdir lib || true"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.3.2",
    "@types/bcrypt": "^1.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/graphql": "^0.12.4",
    "@types/helmet": "0.0.37",
    "@types/jest": "^21.1.8",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.104",
    "@types/lru-cache": "^4.1.0",
    "@types/mime": "^2.0.0",
    "@types/node": "^8.0.53",
    "@types/uuid": "^3.4.3",
    "@types/zen-observable": "^0.5.3",
    "apollo-tracing": "0.0.9",
    "commander": "^2.15.1",
    "concurrently": "^3.5.1",
    "faker": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^21.0.1",
    "nock": "^9.1.0",
    "prettier": "^1.8.2",
    "ts-jest": "^21.2.4",
    "ts-loader": "^3.0.3",
    "tslint": "^5.7.0",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.10",
    "apollo-client": "^2.2.7",
    "apollo-client-preset": "^1.0.8",
    "apollo-link": "^1.0.3",
    "apollo-link-context": "^1.0.7",
    "apollo-link-http": "^1.5.3",
    "apollo-server-express": "^1.2.0",
    "aws-sdk": "^2.156.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dtrace-provider": "^0.8.5",
    "express": "^4.15.5",
    "firebase-admin": "~5.10.0",
    "firebase-functions": "^0.8.2",
    "form-data": "^2.3.1",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.21.0",
    "helmet": "^3.9.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.2",
    "mime": "^2.0.3",
    "moment": "^2.19.2",
    "nodemon": "^1.15.0",
    "parse-domain": "^1.1.0",
    "slug": "^0.9.1",
    "source-map-support": "^0.5.0",
    "uuid": "^3.1.0"
  },
  "repository": "https://git.master-technology.com/nstudio/sketchpoints"
}
