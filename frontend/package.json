    "core-js": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "firebase": "^4.10.0",
    "graphql": "0.13.1",
    "graphql-tag": "^2.8.0",
    "ionicons": "^3.0.0",
    "nativescript-angular": "5.2.0",
    "nativescript-fancyalert": "^1.1.2",
    "nativescript-image-zoom": "file:xplat/nativescript/plugins/nativescript-image-zoom-1.1.1.tgz",
    "nativescript-loading-indicator": "^2.4.0",
    "nativescript-ngx-fonticon": "^4.1.0",
