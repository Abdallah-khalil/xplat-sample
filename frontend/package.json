{
  "name": "sketchpoints",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run start.web",
    "start.web": "ng serve --app=web-viewer",
    "start.nativescript.artist.ios": "cd apps/nativescript-artist && tns run ios --emulator --syncAllFiles",
    "start.nativescript.viewer.ios": "cd apps/nativescript-viewer && tns run ios --emulator --syncAllFiles",
    "start.nativescript.viewer.android": "cd apps/nativescript-viewer && tns run android --emulator --syncAllFiles",
    "build.web": "ng build --app=web-viewer -prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "apps:affected": "./node_modules/.bin/nx affected apps",
    "build:affected": "./node_modules/.bin/nx affected build",
    "e2e:affected": "./node_modules/.bin/nx affected e2e",
    "affected:apps": "./node_modules/.bin/nx affected apps",
    "affected:build": "./node_modules/.bin/nx affected build",
    "affected:e2e": "./node_modules/.bin/nx affected e2e",
    "format": "./node_modules/.bin/nx format write",
    "format:write": "./node_modules/.bin/nx format write",
    "format:check": "./node_modules/.bin/nx format check",
    "nx-migrate": "./node_modules/.bin/nx migrate",
    "nx-migrate:check": "./node_modules/.bin/nx migrate check",
    "nx-migrate:skip": "./node_modules/.bin/nx migrate skip",
    "postinstall": "./node_modules/.bin/nx migrate check",
    "clean": "rm -rf hooks node_modules package-lock.json && npm i",
    "clean.nativescript.artist": "cd apps/nativescript-artist && rm -rf hooks node_modules platforms package-lock.json && npm i",
    "clean.nativescript.viewer": "cd apps/nativescript-viewer && rm -rf hooks node_modules platforms package-lock.json && npm i",
    "clean.shared": "npx rimraf -- libs/**/*.js && npx rimraf -- libs/**/*.map && npx rimraf -- libs/**/*.d.ts && npx rimraf -- libs/**/*.ngfactory.ts && npx rimraf -- libs/**/*.ngsummary.json && npx rimraf -- libs/**/*.metadata.json && npx rimraf -- xplat/**/*.js && npx rimraf -- xplat/**/*.map && npx rimraf -- xplat/**/*.d.ts && npx rimraf -- xplat/**/*.ngfactory.ts && npx rimraf -- xplat/**/*.ngsummary.json && npx rimraf -- xplat/**/*.metadata.json",
    "deploy.pwa": "rm -rf firebase.json && cp firebase.pwa.json firebase.json || true",
    "deploy.pwa.dev": "ng build --app=web-viewer && npm run deploy.pwa && firebase use pwa-dev && firebase deploy || true",
    "deploy.pwa.prod": "ng build --app=web-viewer -prod && npm run deploy.pwa && firebase use pwa-prod && firebase deploy || true",
    "start.admin": "electron ./node_modules/@nstudio/schematics/admin"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.1",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/flex-layout": "5.0.0-beta.13",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.9",
    "@angular/material": "^5.2.1",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.9",
    "@angular/router": "^5.2.0",
    "@angular/service-worker": "^5.2.9",
    "@ngrx/effects": "^5.1.0",
    "@ngrx/router-store": "^5.0.1",
    "@ngrx/store": "^5.1.0",
    "@ngrx/store-devtools": "^5.1.0",
    "@ngx-translate/core": "~9.0.2",
    "@nrwl/nx": "0.7.3",
    "@sketchpoints/scss": "file:libs/scss",
